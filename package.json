{
    "name": "@nodecfdi/cfdi-expresiones",
    "version": "2.1.4",
    "description": "Librería que contiene objetos de ayuda para crear expresiones de CFDI 3.2, CFDI 3.3, CFDI 4.0, RET 2.0 y RET 1.0 de acuerdo a la información técnica del SAT en el Anexo 20.",
    "keywords": [
        "CFDI 4.0",
        "CFDI 3.2",
        "CFDI 3.3",
        "RET 1.0",
        "RET 2.0",
        "Expresion",
        "SAT",
        "Codigo QR"
    ],
    "private": false,
    "author": "OcelotlStudio <contacto@ocelotlstudio.com>",
    "license": "MIT",
    "type": "module",
    "main": "dist/cfdi-expresiones.js",
    "module": "dist/cfdi-expresiones.mjs",
    "unpkg": "dist/cfdi-expresiones.global.js",
    "browser": "dist/cfdi-expresiones.global.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/cfdi-expresiones.mjs",
            "require": "./dist/cfdi-expresiones.js",
            "default": "./dist/cfdi-expresiones.js"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "preinstall": "npx only-allow pnpm",
        "prepare": "is-ci || husky install",
        "lint": "eslint --fix --ext .js,.ts .",
        "lint:check": "eslint --ext .js,.ts .",
        "format": "prettier --write \"**/*.{js,ts,json}\"",
        "test": "vitest",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage",
        "build": "pnpm prebuild && rollup -c && pnpm gen:types && rimraf dist/dts",
        "prebuild": "rimraf dist",
        "gen:docs": "rimraf docs && typedoc --options typedoc.json",
        "gen:types": "api-extractor run --local",
        "release": "pnpm run build && pnpm changeset publish"
    },
    "repository": "https://github.com/nodecfdi/cfdi-expresiones",
    "bugs": {
        "url": "https://github.com/nodecfdi/cfdi-expresiones/issues"
    },
    "homepage": "https://github.com/nodecfdi/cfdi-expresiones",
    "devDependencies": {
        "@changesets/cli": "^2.26.0",
        "@commitlint/cli": "^17.4.4",
        "@microsoft/api-extractor": "^7.34.4",
        "@nrsk/config-conventional": "^1.0.0",
        "@rollup/plugin-commonjs": "^24.0.1",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-terser": "^0.4.0",
        "@rollup/plugin-typescript": "^11.0.0",
        "@types/is-ci": "^3.0.0",
        "@types/node": "^18.14.0",
        "@typescript-eslint/eslint-plugin": "^5.53.0",
        "@typescript-eslint/parser": "^5.53.0",
        "@vitest/coverage-istanbul": "^0.29.0",
        "@xmldom/xmldom": "^0.8.6",
        "eslint": "^8.34.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-config-xo": "^0.43.1",
        "eslint-config-xo-typescript": "^0.56.0",
        "eslint-plugin-tsdoc": "^0.2.17",
        "eslint-plugin-unicorn": "^45.0.2",
        "husky": "^8.0.3",
        "is-ci": "^3.0.1",
        "jsdom": "^21.1.0",
        "lint-staged": "^13.1.2",
        "prettier": "^2.8.4",
        "rimraf": "^4.1.2",
        "rollup": "^3.17.2",
        "ts-mixer": "^6.0.3",
        "typedoc": "^0.23.25",
        "typedoc-theme-hierarchy": "^3.0.2",
        "typescript": "^4.9.5",
        "vite": "^4.1.4",
        "vitest": "^0.29.0"
    },
    "dependencies": {
        "@nodecfdi/cfdiutils-common": "^1.2.3"
    },
    "commitlint": {
        "extends": [
            "@nrsk/config-conventional"
        ]
    },
    "lint-staged": {
        "*.{js,ts,json}": [
            "prettier --write"
        ],
        "*.{js,ts}": [
            "eslint --fix"
        ]
    },
    "engines": {
        "node": ">= 14.16.0"
    },
    "browserslist": [
        "defaults",
        "not IE 11"
    ]
}
