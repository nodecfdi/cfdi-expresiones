{
  "name": "@nodecfdi/cfdi-expresiones",
  "version": "2.1.7",
  "description": "Librería que contiene objetos de ayuda para crear expresiones de CFDI 3.2, CFDI 3.3, CFDI 4.0, RET 2.0 y RET 1.0 de acuerdo a la información técnica del SAT en el Anexo 20.",
  "keywords": [
    "CFDI 4.0",
    "CFDI 3.2",
    "CFDI 3.3",
    "RET 1.0",
    "RET 2.0",
    "Expresion",
    "SAT",
    "Codigo QR"
  ],
  "private": false,
  "author": "OcelotlStudio <contacto@ocelotlstudio.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/cfdi-expresiones.cjs",
  "module": "./dist/cfdi-expresiones.js",
  "unpkg": "./dist/cfdi-expresiones.global.js",
  "browser": "./dist/cfdi-expresiones.global.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/cfdi-expresiones.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/cfdi-expresiones.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "husky",
    "lint": "xo --fix",
    "lint:check": "xo",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "build": "rimraf dist && tsup",
    "gen:docs": "rimraf docs && typedoc --options typedoc.json",
    "release": "pnpm run build && pnpm changeset publish"
  },
  "repository": "https://github.com/nodecfdi/cfdi-expresiones",
  "bugs": {
    "url": "https://github.com/nodecfdi/cfdi-expresiones/issues"
  },
  "homepage": "https://github.com/nodecfdi/cfdi-expresiones",
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^19.0.1",
    "@commitlint/config-conventional": "^19.0.0",
    "@nodecfdi/cfdiutils-common": "^2.0.1",
    "@vitest/coverage-istanbul": "^1.3.1",
    "@xmldom/xmldom": "^0.8.10",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typedoc": "^0.25.9",
    "typedoc-material-theme": "^1.0.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
}
