{
  "name": "@nodecfdi/cfdi-expresiones",
  "version": "2.1.7",
  "description": "Librería que contiene objetos de ayuda para crear expresiones de CFDI 3.2, CFDI 3.3, CFDI 4.0, RET 2.0 y RET 1.0 de acuerdo a la información técnica del SAT en el Anexo 20.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "imports": {
    "#src/*": "./src/*.js",
    "#tests/*": "./tests/*.js"
  },
  "scripts": {
    "clean": "del-cli dist",
    "prepare": "is-in-ci || husky",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prebuild": "pnpm clean",
    "build": "tsup",
    "gen:docs": "del-cli docs && typedoc --options typedoc.json",
    "version": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s",
    "postversion": "pnpm format && git add CHANGELOG.md",
    "release": "pnpm run build && pnpm changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@nodecfdi/cfdi-core": "^0.1.0",
    "@nodecfdi/eslint-config": "^1.6.6",
    "@nodecfdi/prettier-config": "^1.1.1",
    "@nodecfdi/tsconfig": "^1.5.0",
    "@types/node": "^20.12.7",
    "@vitest/coverage-istanbul": "^1.5.2",
    "conventional-changelog-cli": "^4.1.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.0",
    "eslint-define-config": "^2.1.0",
    "husky": "^9.0.11",
    "is-in-ci": "^0.1.0",
    "prettier": "^3.2.5",
    "tsup": "^8.0.2",
    "typedoc": "^0.25.13",
    "typedoc-material-theme": "^1.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.2"
  },
  "peerDependencies": {
    "@nodecfdi/cfdi-core": "^0.1.0"
  },
  "author": "Fernando Isidro <luffynando@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/nodecfdi/cfdi-expresiones",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodecfdi/cfdi-expresiones.git"
  },
  "bugs": {
    "url": "https://github.com/nodecfdi/cfdi-expresiones/issues"
  },
  "keywords": [
    "CFDI 4.0",
    "CFDI 3.2",
    "CFDI 3.3",
    "RET 1.0",
    "RET 2.0",
    "Expresion",
    "SAT",
    "Codigo QR"
  ],
  "engines": {
    "node": "^21 || ^20 || ^19 || ^18 || ^16"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ],
  "prettier": "@nodecfdi/prettier-config"
}
